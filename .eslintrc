{
  "env": {
  "browser": true,
  "es2020": true,
  "jest": true
  },
  "extends": [
    "airbnb"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": [2, {
      "js": "always"
    }]
  }
}
